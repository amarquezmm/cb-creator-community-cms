{
  "kind": "collectionType",
  "collectionName": "past_events_highlights",
  "info": {
    "singularName": "past-event-highlight",
    "pluralName": "past-events-highlights",
    "displayName": "Past Events Highlights"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "eventThumb": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "shortDescription": {
      "type": "text"
    },
    "event_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event-categories.event-category"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "pastEventContent": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text",
        "shared.media"
      ]
    }
  }
}
